{
  "name": "integrator",
  "description": "An experiment in fixing integration testing.",
  "bin": "bin/run",
  "main": "out/integrator",
  "scripts": {
    "build": "babel --optional runtime src --out-dir out",
    "watch": "babel --watch --optional runtime src --out-dir out",
    "test": "npm run lint && npm run specs",
    "specs": "./bin/test --suite src/test/test-integrator-actions",
    "lint": "eslint src/**/*.js -c .eslintrc",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/phuu/integrator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phuu/integrator/issues"
  },
  "homepage": "https://github.com/phuu/integrator",
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "eslint": "^1.6.0",
    "ghooks": "^1.0.1",
    "nodemon": "^1.3.7",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "babel": "^5.8.23",
    "babel-runtime": "^6.0.14",
    "chalk": "^1.0.0",
    "fnkit": "^1.0.1",
    "immutable": "^3.7.3",
    "integrator-match": "^1.1.0",
    "leadfoot": "^1.4.1",
    "minimist": "^1.1.1",
    "typd": "^3.0.0"
  }
}
